TARGETS = $(BUILD_DIR)/bin/llvm-dwp.exe

include $(TOP_DIR)/Common.Windows

ALL_SOURCES = DWPError.cpp \
              llvm-dwp.cpp

OBJECTS = $(call cpp_to_obj, $(ALL_SOURCES))

