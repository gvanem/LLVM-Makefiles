#
# GNU Makefile for 'clang/lib/AST'.
#
TOP_DIR = ../../..
TARGETS = $(BUILD_DIR)/lib/clangAST.lib

VPATH = Interp

include $(TOP_DIR)/Common.Windows

#
# .inc-files needed here:
#
